<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<VBox fx:id="root"
      fx:controller="com.ticTacToeGame.MainViewController"
      xmlns:fx="http://javafx.com/fxml"
      alignment="CENTER"
      spacing="20.0">
    <ImageView fitWidth="${root.width}"
               fitHeight="${root.height}">
        <Image url="@images/tic_tac_toe_ic.gif"/>
    </ImageView>

    <padding>
        <Insets top="20.0"
                right="20.0"
                bottom="20.0"
                left="20.0"/>
    </padding>
</VBox>
